<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--
#set( $filename = "${project}.launch" )
-->
<launchConfiguration type="org.eclipse.pde.ui.EquinoxLauncher">
	<booleanAttribute key="append.args" value="true" />
	<booleanAttribute key="automaticAdd" value="false" />
	<booleanAttribute key="automaticValidate" value="false" />
	
	<stringAttribute key="bootstrap" value="" />
	<stringAttribute key="checked" value="[NONE]" />
	
	<booleanAttribute key="clearConfig" value="false" />
	<stringAttribute key="configLocation"
		value="${d}{workspace_loc}/.metadata/.plugins/org.eclipse.pde.core/${project}" />
		
	<booleanAttribute key="default_auto_start" value="true" />
	<intAttribute key="default_start_level" value="4" />
	
	<stringAttribute key="org.eclipse.jdt.launching.PROGRAM_ARGUMENTS"
		value="-os ${d}{target.os} -ws ${d}{target.ws} -arch ${d}{target.arch} -nl ${d}{target.nl} -console" />
	<stringAttribute key="org.eclipse.jdt.launching.SOURCE_PATH_PROVIDER"
		value="org.eclipse.pde.ui.workbenchClasspathProvider" />
	<stringAttribute key="org.eclipse.jdt.launching.VM_ARGUMENTS"
		value="-Declipse.ignoreApp=true -Dosgi.noShutdown=true -Dorg.eclipse.equinox.http.jetty.http.port=8080" />
		
	<stringAttribute key="pde.version" value="3.3" />
	
	<booleanAttribute key="show_selected_only" value="false" />
	
	<stringAttribute key="target_bundles"
		value="org.mortbay.jetty.server@default:default,org.eclipse.osgi@-1:true,org.eclipse.equinox.common@2:true,org.eclipse.equinox.log@default:default,org.eclipse.equinox.http.servlet@default:default,org.eclipse.core.contenttype@default:default,org.eclipse.riena.communication.core@default:default,org.eclipse.riena.communication.factory.hessian@default:default,org.eclipse.equinox.preferences@default:default,org.eclipse.equinox.http.jetty@default:default,org.eclipse.riena.communication.publisher.hessian@default:default,org.eclipse.equinox.app@default:default,org.apache.log4j@default:default,org.eclipse.riena.communication.publisher@default:default,org.eclipse.equinox.http.registry@default:default,org.eclipse.riena.core@default:default,org.eclipse.osgi.services@default:default,org.eclipse.core.jobs@default:default,org.mortbay.jetty.util@default:default,com.caucho.hessian@default:default,javax.servlet@default:default,org.eclipse.equinox.registry@default:default,org.eclipse.core.variables@default:default,org.eclipse.core.runtime@default:true" />
	<booleanAttribute key="tracing" value="false" />
	<booleanAttribute key="useDefaultConfigArea" value="true" />
	<stringAttribute key="workspace_bundles"
		value="${plugin}@default:default,${plugin.common}@default:default" />
</launchConfiguration>
